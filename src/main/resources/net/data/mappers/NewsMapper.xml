<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.data.mappers.NewsMapper">

	<insert id="addNews" parameterType="News" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO NEWS(title,content,post_date,author,status,folder_id,media_url,media,thumbnails_url)
		VALUES(#{title},#{content},#{postDate},#{author},#{status},#{folderId},#{mediaUrl},#{media},#{thumbnailsUrl})
	</insert>

</mapper>